                <div class="row col-md-6">
                    <div class="form-floating col-md-8" style="text-align:right;">
                        <label for="ChargesDescription" style="margin-left:7px;">@WebResource.ChargesDescription</label>
                    </div>
                    <div class="form-floating col-md-4">
                        <input type="text" name="OtherCharges" id="OtherCharges" class="form-control" value="@Model.OtherCharges" data-val="@Model.OtherCharges" style="text-align:right;" readonly />

        function ShowCurrentInvoiceDoc() {
            var docPath = "@invoicePath";
            var object = "<object data=\"{FileName}\" type=\"application/pdf\" width=\"500px\" height=\"300px\">";
            object += "</object>";
            object = object.replace(/{FileName}/g, docPath);


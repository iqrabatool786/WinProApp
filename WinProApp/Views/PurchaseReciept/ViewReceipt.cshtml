            if (parseFloat(prev) > 0) {
                prev = prev;
            }
            else {
                prev = 0;
            }

            if (parseFloat(curVat) > 0) {
                curVat = curVat;
            }
            else {
                curVat = 0;
            }

            if (parseFloat(tot) > 0) {
                tot = tot;
            }
            else {

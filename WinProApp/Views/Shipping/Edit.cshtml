            if(parseInt(Id) > 0){
            $.ajax({
                url: "/Purchase/GetSupplierInfo/" + Id,
                type: "GET",
                success: function (data) {
                    var result = JSON.parse(JSON.stringify(data));
                    $("#BpCode").val(result.Bpcode);
                    $("#Address").val(result.Address);
                    $("#VatNo").val(result.VatNo);
                    $('#Balance').val(result.Balance);
                },

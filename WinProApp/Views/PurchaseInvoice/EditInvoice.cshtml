                    <label for="txtStock">@WebResource.Stock</label>
                    <input type="text" id="txtStock" name="txtStock" class="form-control" readonly />
                </div>
            </div>

            <div class="row" style="margin-top:10px;">
                <div class="form-floating col-md-1">

        function ValidateAddRow() {
            let isAdd = true;
            if(!$('#txtBarcode').val()) {
                $('.valTxtBarcode').show();
                isAdd = false;
            } else {
                $('.valTxtBarcode').hide();
            }

            if (!$('#txtDesc').val() && !$('#txtDesc1').val()) {
                $('.valDescEng').show();
                $('.valDescAr').show();
                isAdd = false;
            } else {
                $('.valDescEng').hide();
                $('.valDescAr').hide();
            }

            if ((!$('#txtQtyD').val() || parseFloat($('#txtQtyD').val()) <= 0) && (!$('#txtQtyP').val() || parseFloat($('#txtQtyP').val()) <= 0) ) {
                $('.valQtyd').show();
                $('.valQtyp').show();
                isAdd = false;
            } else {
                $('.valQtyd').hide();
                $('.valQtyp').hide();
            }

            if(!$('#txtCost').val()) {
                $('.valTxtCost').show();
                isAdd = false;
            } else {
                $('.valTxtCost').hide();
            }

            if(!$('#txtSelling').val()) {
                $('.valTxtSelling').show();
                isAdd = false;
            } else {
                $('.valTxtSelling').hide();
            }

            return isAdd;
        }

